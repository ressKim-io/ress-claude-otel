      "targets": [
        {
          "datasource": { "type": "loki", "uid": "loki" },
          "expr": "100 * (sum by (tool_name)(count_over_time({service_name=\"claude-code\"} | event_name=\"tool_result\" | success=\"true\" [15m]))) / (sum by (tool_name)(count_over_time({service_name=\"claude-code\"} | event_name=\"tool_result\" [15m])))",
          "legendFormat": "{{tool_name}}"
        }
      ]
    },
